//HappyNumber
class MagicNumber 
{
	public static void main(String[] args) 
	{
		int num=1234;

		while(num>9){
			int sum=0;
			while(num>0){
				sum=+num%10;
				num/=10;
			}
			num=sum;
		}

		if(sum%10==1)
			System.out.println("Happy Number");
		else
			System.out.println("not a Happy Number");

	}
}
